-static NTSTATUS pulse_get_latency(void *args)
-{
-    struct get_latency_params *params = args;
-    struct pulse_stream *stream = params->stream;
-    const pa_buffer_attr *attr;
-    REFERENCE_TIME lat;
-
-    pulse_lock();

+        total_len = FIELD_OFFSET(typeof(*buffer), MountPointReparseBuffer.PathBuffer[path_len/sizeof(WCHAR)]);
+        buffer->SymbolicLinkReparseBuffer.Flags = flags;
+        break;
+    default:
